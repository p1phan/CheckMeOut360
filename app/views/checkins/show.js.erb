$('li#li_for_wall').removeClass("active");
$('li#li_for_place').removeClass("active");
$('#yield_for_main_content').empty();
$('#yield_for_main_content').html('<%= strip_new_line(render("show_checkin")).html_safe %>');

init_google_map("<%=@checkin.place.lat%>", "<%=@checkin.place.long%>");
marker = drop_single_marker("<%=@checkin.place.lat%>", "<%=@checkin.place.long%>", "<h3><%=@checkin.place.name%></h3><h4>Been here <%=@checkin.place.count%> times.</h4>");
google.maps.event.addListener(marker, 'click', function (a) {
  add_marker_window_listener(marker);
});

