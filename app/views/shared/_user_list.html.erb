<% if current_user %>
  <%= link_to current_user.email, wall_path(current_user.wall.wall_name), :class => "user_list btn primary bottom_space", :id =>"user_listing" %>
<% end %>
<% @users.each do |user| %>
  <%= link_to user.email, wall_path(user.wall.wall_name), :class => "user_list btn info bottom_space", :id =>"user_listing" %>
<% end %>
