- create a graph API access
@graph = Koala::Facebook::API.new(token)

- get checkins
@graph.get_connections("me", "checkins")






@graph = Koala::Facebook::API.new(User.first.token)
x = @graph.get_connections("me", "checkins")
c = Facebook::Checkin.new x.first
fb = Checkin::build_checkins([c])

